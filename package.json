  "private": true,
  "gcloud": {
    "id": "pose-playground",
    "version": "1",
    "devPort": 8080,
    "devAdminPort": 8000
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^0.13.0",
